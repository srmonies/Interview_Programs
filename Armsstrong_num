package com.interviewprograms;

public class Armsstrong_num {
	public static void main(String[] args) {
		int n = 153, temp, arms = 0,orginal=n;
		while (n>0) {
		temp = n % 10;
		temp = (int) Math.pow(temp, 3);
		arms = arms + temp;
		n = n / 10;
		}
		if (arms == orginal) {
			System.out.println("It is armsstrong num");
		} else {
			System.out.println("not armsstrong num");
		}
	}
	

}
