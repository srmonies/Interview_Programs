package com.interviewprograms;

public class Palandrome {

	public static void main(String[] args) {
		int n = 121;
		int temp = n;
		int rev = 0;
		while (n > 0) 
		{
		int b = n % 10;
			rev = rev * 10 + b;
			n = n / 10;
	}
		if (temp == rev) {
			System.out.println("It is palandrome");
		} else {
			System.out.println("not a palandrome");
		}
	}

}
